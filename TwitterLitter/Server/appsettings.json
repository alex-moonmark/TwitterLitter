{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TwitterAPI": {
    "BearerToken": "[Bearer Token]",
    "StreamUri": "https://api.twitter.com/2/tweets/sample/stream",
    "User-Agent": "TwitterLitter",
    "StreamOptions": "?tweet.fields=created_at&expansions=author_id&user.fields=created_at",
    "HashtagRegex": "#(\\w*[a-zA-Z]+\\w*[0-9]+)|#(\\w*[0-9]*\\w*[a-zA-Z]+)"

  }
}
